{
    "type": "project",
    "license": "proprietary",
    "name": "survos-sites/tree",
    "description": "Demo project for jstree and survos/tree-bundle  ",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "^4.0",
        "composer/package-versions-deprecated": ">=1.11.99.5",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^3.2",
        "doctrine/persistence": "^3.0.0||^4.0",
        "friendsofsymfony/jsrouting-bundle": "^3.1.1",
        "league/commonmark": "^2.3",
        "nelmio/cors-bundle": "^2.1",
        "phpdocumentor/reflection-docblock": "^5.2",
        "stof/doctrine-extensions-bundle": "^1.4",
        "survos/bootstrap-bundle": "^1.4",
        "survos/core-bundle": "*@dev",
        "survos/crawler-bundle": "^1.4",
        "survos/inspection-bundle": "^1.4",
        "survos/tree-bundle": "^1.5",
        "symfony/asset": "^7.1",
        "symfony/asset-mapper": "^7.1",
        "symfony/console": "^7.1",
        "symfony/dotenv": "^7.1",
        "symfony/error-handler": "^7.1",
        "symfony/expression-language": "^7.1",
        "symfony/flex": "^2.0",
        "symfony/form": "^7.1",
        "symfony/framework-bundle": "^7.1",
        "symfony/http-client": "^7.1",
        "symfony/intl": "^7.1",
        "symfony/mailer": "^7.1",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "^7.1",
        "symfony/process": "^7.1",
        "symfony/property-access": "^7.1",
        "symfony/property-info": "^7.1",
        "symfony/runtime": "^7.1",
        "symfony/security-bundle": "^7.1",
        "symfony/serializer": "^7.1",
        "symfony/stimulus-bundle": "^2.x-dev",
        "symfony/string": "^7.1",
        "symfony/translation": "^7.1",
        "symfony/twig-bundle": "^7.1",
        "symfony/uid": "^7.1",
        "symfony/validator": "^7.1",
        "symfony/web-link": "^7.1",
        "symfony/yaml": "^7.1",
        "tacman/twig-tree-tag": "^3.0",
        "twig/cssinliner-extra": "^3.0",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/inky-extra": "^3.0",
        "twig/intl-extra": "^3.4",
        "twig/markdown-extra": "^3.0",
        "twig/string-extra": "^3.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3||^4.0",
        "hgtan/symfony-pre-commit": "dev-master",
        "phpunit/phpunit": "^11.2",
        "rector/rector": "^2.0",
        "roave/security-advisories": "dev-latest",
        "survos/deployment-bundle": "^1.5",
        "survos/maker-bundle": "^1.5",
        "symfony/browser-kit": "^7.1",
        "symfony/css-selector": "^7.1",
        "symfony/debug-bundle": "^7.1",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^7.1",
        "symfony/stopwatch": "^7.1",
        "symfony/web-profiler-bundle": "^7.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "endroid/installer": true,
            "survos/installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "pre-update-cmd": "# vendor/bin/phpstan analyse src/Entity",
        "pre-commit-cmd": [
            "bin/console lint:twig",
            "# add this to endpoint? /home/tac/g/survos/survos/packages/recipes/index.json",
            "php -v"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "fos:js-routing:dump --format=json --target=public/js/fos_js_routes.json": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "lint": [
            "bin/console lint:twig"
        ],
        "compile": [
            "bin/console doctrine:migrations:migrate -n --allow-no-migration ",
            "# bin/console app:import-topics"
        ],

        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^7.3"
        }
    },
    "prefer-stable": true
}
